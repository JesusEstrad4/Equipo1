from machine import Pin, ADC  
from time import sleep_ms  
  
potX=ADC(26)  
potY=ADC(27)  
  
while True:  
    valX= potX.read_u16()  
    valY= potY.read_u16()  
    print("ValX: ", valX, " ValY: ", valY)  
    sleep_ms(200)  
  
  
  
import machine  
import utime  
led = machine.Pin(25, machine.Pin.OUT)  
  
while True:  
  led.value(1)  
  utime.sleep(1)  
  led.value(0)
  utime.sleep(1)
